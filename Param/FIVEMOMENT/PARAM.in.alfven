#GRID
1			nRootBlock1
1			nRootBlock2
1			nRootBlock3
-10000.			xMin
 10000.			xMax
-1.			yMin
 1.			yMax
-1.			zMin
 1.			zMax

#OUTERBOUNDARY
periodic		TypeBc1
periodic		TypeBc2

#USERINPUTBEGIN -----------------------------------

Bx  = 1
Rho = 1
cA  = 1
Bz  = 0
Uz  = 0

#WAVE
By			By_       = Ampl*cos(2pi/LambdaX*x)
10000.0			Width
0.1			Amplitude = 0.1
10000.0			LambdaX   = 1e4
-1.0			LambdaY
-1.0			LambdaZ
0.0			Phase

#WAVE
Uy			Uy_ = cA * By / Bx
10000.0			Width
0.1			Amplitude
10000.0			LambdaX
-1.0			LambdaY
-1.0			LambdaZ
0.0			Phase

#WAVE
ElUy			ElUy_ = Uey = Uy  since Jy=0
10000.0			Width
0.1			Amplitude
10000.0			LambdaX
-1.0			LambdaY
-1.0			LambdaZ
0.0			Phase

#WAVE
ElUz			ElUz_ = Uez = Uz - Jz/(n*e) = -dBy/dx / (Rho/Mi)
10000.0			Width       = 2pi/LambdaX*Ampl*sin(2pi/LambdaX*x)
6.2831852e-05		Amplitude     2e-5*pi
10000.0			LambdaX
-1.0			LambdaY
-1.0			LambdaZ
-90.0			Phase       sin(x) = cos(x-90)

#WAVE
Ey			Ey_  = -Uez * Bx = -2pi/LambdaX*Ampl*sin(2pi/LambdaX*x)
10000.0			Width
-6.2831852e-05		Amplitude 2e-4*pi
10000.0			LambdaX
-1.0			LambdaY
-1.0			LambdaZ
-90.0			Phase

#WAVE
Ez			Ez_ = +Uey * Bx
10000.0			Width
0.1			Amplitude
10000.0			LambdaX
-1.0			LambdaY
-1.0			LambdaZ
0.0			Phase

#USERINPUTEND -------------------------------------

#NONCONSERVATIVE
T			UseNonConservative

#SCHEME
2			nOrder
Rusanov			TypeFlux
mc			TypeLimiter
1.5			LimiterBeta

#TIMESTEPPING
